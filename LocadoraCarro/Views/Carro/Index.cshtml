@model IList<LocadoraCarro.Models.Carro>
<h1>Carro</h1>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Descrição</th>
        </tr>
    </thead>
    <tbody>       
        @foreach (var carro in Model)
        {
            <tr>
                <td>@carro.Id</td>
                <td>
                    @Html.ActionLink(carro.Descricao, "Visualiza", new { id = carro.Id } )
                </td>
            </tr>
        }            
    </tbody>
</table>
@Html.ActionLink("Adicionar", "Form", "Carro")

